{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/",
    "test": "deno run -A --node-modules-dir npm:vitest run",
    "test:watch": "deno run -A --node-modules-dir npm:vitest"
  },
  "compilerOptions": {
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@/": "./src/",
    "@bufbuild/protobuf": "npm:@bufbuild/protobuf@^2.6.3",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.10",
    "@tanstack/eslint-plugin-query": "npm:@tanstack/eslint-plugin-query@^5.83.1",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.84.2",
    "vitest": "npm:vitest@^2",
    "@testing-library/react": "npm:@testing-library/react@^16",
    "@testing-library/jest-dom": "npm:@testing-library/jest-dom@^6",
    "jsdom": "npm:jsdom@^25",
    "@tanstack/react-query-devtools": "npm:@tanstack/react-query-devtools@^5.84.2",
    "@types/react": "npm:@types/react@^19.0.10",
    "@types/react-dom": "npm:@types/react-dom@^19.0.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "daisyui": "npm:daisyui@^5.0.43",
    "lucide-react": "npm:lucide-react@^0.522.0",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "react-hook-form": "npm:react-hook-form@^7.58.1",
    "react-router-dom": "npm:react-router-dom@^7.6.2",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "vite": "npm:vite@^6.1.1",
    "zod": "npm:zod@^3.25.67"
  }
}